<UserControl
    x:Class="SampleWinui.View.ScheduleListItems"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:controls1="using:CommunityToolkit.WinUI.UI.Controls"
    xmlns:converter="using:SampleWinui.Converter"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gl="using:Windows.Globalization"
    xmlns:local="using:SampleWinui.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <UserControl.Resources>
        <converter:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </UserControl.Resources>
    <Grid
        Width="800"
        Margin="20,20,0,0"
        Background="White">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid
            Grid.Row="0"
            Margin="10,12,10,10"
            BorderBrush="#bdbdbd"
            BorderThickness="1"
            CornerRadius="6">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Margin="10">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <RadioButton
                    x:Name="dayOnly"
                    Grid.Row="0"
                    Margin="10,5,5,5"
                    VerticalAlignment="Center"
                    Command="{Binding Filter}"
                    CommandParameter="dayOnly"
                    Content="One day only"
                    FlowDirection="LeftToRight"
                    IsChecked="True" />

                <RadioButton
                    x:Name="dayOfWeek"
                    Grid.Row="1"
                    Margin="10,5,5,5"
                    VerticalAlignment="Center"
                    Command="{Binding Filter}"
                    CommandParameter="dayOfWeek"
                    Content="By day of week"
                    FlowDirection="LeftToRight" />
                <RadioButton
                    x:Name="dayOfMonth"
                    Grid.Row="2"
                    Margin="10,5,5,5"
                    VerticalAlignment="Center"
                    Command="{Binding Filter}"
                    CommandParameter="dayOfMonth"
                    Content="By day of month"
                    FlowDirection="LeftToRight" />
                <CheckBox
                    Grid.Row="3"
                    Margin="10,5,5,5"
                    Content="Enabled"
                    FlowDirection="LeftToRight" />
            </Grid>
            <Grid Grid.Column="1" Margin="10">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <CalendarDatePicker
                    x:Name="MyCalendarDatePicker"
                    Width="200"
                    Margin="0,10,5,5"
                    BorderBrush="#bdbdbd"
                    BorderThickness="1"
                    CalendarIdentifier="GregorianCalendar"
                    CornerRadius="15"
                    DateFormat="{}{month.abbreviated} {day.integer}, {year.full}"
                    Visibility="{Binding DatePickEnable, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <Grid Grid.Row="2">
                    <TextBox
                        Width="40"
                        Margin="0,50,0,0"
                        Padding="20,4,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Center"
                        BorderBrush="#bdbdbd"
                        BorderThickness="1"
                        CornerRadius="12"
                        Text="28"
                        Visibility="{Binding DayOfMonthEnable, Converter={StaticResource BooleanToVisibilityConverter}}" />
                </Grid>
            </Grid>
            <Grid
                Grid.Column="2"
                Margin="10"
                HorizontalAlignment="Right"
                Visibility="{Binding EnableWeeks, Converter={StaticResource BooleanToVisibilityConverter}}">
                <ListView ItemsSource="{Binding WeekCollection}">
                    <ListView.ItemTemplate>
                        <DataTemplate>
                            <CheckBox Content="{Binding Day}" FlowDirection="LeftToRight" />
                        </DataTemplate>
                    </ListView.ItemTemplate>
                    <ListView.ItemsPanel>
                        <ItemsPanelTemplate>
                            <ItemsWrapGrid MaximumRowsOrColumns="4" Orientation="Vertical" />
                        </ItemsPanelTemplate>
                    </ListView.ItemsPanel>
                </ListView>
            </Grid>

        </Grid>
        <Grid
            Grid.Row="0"
            Margin="30,0,0,0"
            Padding="5,0,5,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="#fff">
            <TextBlock Text="Date" />
        </Grid>
        <Grid
            Grid.Row="1"
            Height="180"
            Margin="10,12,10,10"
            BorderBrush="#bdbdbd"
            BorderThickness="1"
            CornerRadius="6">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="0" Margin="10">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <RadioButton
                    x:Name="specificTime"
                    Grid.Row="0"
                    Margin="10,5,5,5"
                    VerticalAlignment="Center"
                    Command="{Binding TimeFilter}"
                    CommandParameter="specificTime"
                    Content="Specific time"
                    FlowDirection="LeftToRight" />

                <RadioButton
                    x:Name="autoRecurring"
                    Grid.Row="1"
                    Margin="10,5,5,5"
                    VerticalAlignment="Center"
                    Command="{Binding TimeFilter}"
                    CommandParameter="autoRecurring"
                    Content="Auto recurring"
                    FlowDirection="LeftToRight" />
            </Grid>
            <Grid
                Grid.Column="1"
                Height="40"
                Margin="10,15,10,10"
                VerticalAlignment="Top"
                BorderBrush="#bdbdbd"
                BorderThickness="1"
                CornerRadius="18"
                Visibility="{Binding LeftTimePanel, Converter={StaticResource BooleanToVisibilityConverter}}">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <TimePicker
                    x:Name="MyTimePicker"
                    Grid.Column="0"
                    Margin="0,7,0,0"
                    VerticalAlignment="Top"
                    BorderThickness="0"
                    Time="11:10:45" />
                <Grid
                    Grid.Column="1"
                    Margin="0,8,0,0"
                    HorizontalAlignment="Left"
                    Visibility="Collapsed">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <RepeatButton
                        Name="btnPrev"
                        Height="20"
                        Margin="0,0,0,0"
                        HorizontalContentAlignment="Left"
                        Background="Transparent"
                        BorderThickness="0"
                        Click="IncrementButton_Click">
                        <Path
                            Width="10"
                            Height="5"
                            Margin="0,0,0,0"
                            Data="M0,10 L5,0 L10,10 Z"
                            Fill="Black"
                            RenderTransformOrigin="0.5,0.5"
                            Stretch="Uniform" />
                    </RepeatButton>
                    <RepeatButton
                        Grid.Row="1"
                        Background="Transparent"
                        BorderThickness="0"
                        Click="DecrementButton_Click">
                        <Path
                            Width="10"
                            Height="5"
                            Margin="0,0,0,5"
                            Data="M0,0 L5,10 L10,0 Z"
                            Fill="Black"
                            RenderTransformOrigin="0.5,0.5"
                            Stretch="Uniform" />
                    </RepeatButton>
                </Grid>
            </Grid>
            <Grid
                Grid.Column="2"
                HorizontalAlignment="Right"
                Visibility="{Binding RightTimePanel, Converter={StaticResource BooleanToVisibilityConverter}}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid Margin="10" HorizontalAlignment="Right">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock
                            Margin="8"
                            Style="{StaticResource MainFontStyle}"
                            Text="Every" />
                        <TextBox
                            Width="30"
                            Height="5"
                            Padding="20,4,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            BorderBrush="#bdbdbd"
                            BorderThickness="1"
                            CornerRadius="12"
                            HorizontalTextAlignment="Center"
                            Text="28" />
                        <TextBlock
                            Margin="8"
                            Style="{StaticResource MainFontStyle}"
                            Text="Minutes" />
                    </StackPanel>
                </Grid>
                <Grid Grid.Row="1">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock
                            Margin="8,20,8,10"
                            Style="{StaticResource MainFontStyle}"
                            Text="Starting at" />
                        <Grid
                            Height="40"
                            Margin="10,10,15,10"
                            BorderBrush="#bdbdbd"
                            BorderThickness="1"
                            CornerRadius="18">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TimePicker
                                x:Name="StartingAtTimePicker"
                                Grid.Column="0"
                                Margin="0,7,0,0"
                                VerticalAlignment="Top"
                                HorizontalContentAlignment="Center"
                                BorderThickness="0"
                                Time="11:34:45" />
                            <Grid
                                Grid.Column="1"
                                Margin="0,8,0,0"
                                HorizontalAlignment="Left"
                                Visibility="Collapsed">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <RepeatButton
                                    HorizontalContentAlignment="Left"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    Click="StartingAtIncrementButton_Click">
                                    <Path
                                        Width="10"
                                        Height="5"
                                        Margin="0,0,0,0"
                                        Data="M0,10 L5,0 L10,10 Z"
                                        Fill="Black"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform" />
                                </RepeatButton>
                                <RepeatButton
                                    Grid.Row="1"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    Click="StartingAtDecrementButton_Click">
                                    <Path
                                        Width="10"
                                        Height="5"
                                        Margin="0,0,0,3"
                                        Data="M0,0 L5,10 L10,0 Z"
                                        Fill="Black"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform" />
                                </RepeatButton>
                            </Grid>
                        </Grid>
                    </StackPanel>
                </Grid>
                <Grid Grid.Row="2">
                    <StackPanel Orientation="Horizontal">
                        <TextBlock
                            Margin="8,20,8,10"
                            Style="{StaticResource MainFontStyle}"
                            Text="Ending at" />
                        <Grid
                            Height="40"
                            Margin="15,10,15,10"
                            BorderBrush="#bdbdbd"
                            BorderThickness="1"
                            CornerRadius="18">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TimePicker
                                x:Name="EndingAtTimePicker"
                                Grid.Column="0"
                                Margin="0,7,0,0"
                                VerticalAlignment="Top"
                                HorizontalContentAlignment="Center"
                                BorderThickness="0"
                                Time="10:33:45" />
                            <Grid
                                Grid.Column="1"
                                Margin="0,8,0,0"
                                HorizontalAlignment="Left"
                                Visibility="Collapsed">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <RepeatButton
                                    HorizontalContentAlignment="Left"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    Click="EndingAtIncrementButton_Click">
                                    <Path
                                        Width="10"
                                        Height="5"
                                        Margin="0,0,0,0"
                                        Data="M0,10 L5,0 L10,10 Z"
                                        Fill="Black"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform" />
                                </RepeatButton>
                                <RepeatButton
                                    Grid.Row="1"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    Click="EndingAtDecrementButton_Click">
                                    <Path
                                        Width="10"
                                        Height="5"
                                        Margin="0,0,0,2"
                                        Data="M0,0 L5,10 L10,0 Z"
                                        Fill="Black"
                                        RenderTransformOrigin="0.5,0.5"
                                        Stretch="Uniform" />
                                </RepeatButton>
                            </Grid>
                        </Grid>
                    </StackPanel>
                </Grid>
            </Grid>
        </Grid>
        <Grid
            Grid.Row="1"
            Margin="30,0,0,0"
            Padding="5,0,5,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Background="#fff">
            <TextBlock Text="Time" />
        </Grid>
        <Grid Grid.Row="2" Margin="10,50,10,10">
            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                <Button
                    Width="150"
                    Height="30"
                    HorizontalAlignment="Right"
                    HorizontalContentAlignment="Center"
                    Background="{StaticResource ScheduleListItemBlueButtonBackGround}"
                    Content="Done"
                    CornerRadius="17"
                    FontSize="10"
                    Foreground="#FFFFFF"
                    Style="{StaticResource ButtonStyle}" />
                <Button
                    Width="150"
                    Height="30"
                    Margin="10,0,10,0"
                    HorizontalContentAlignment="Center"
                    Background="{StaticResource ScheduleListItemGreenButtonBackGround}"
                    Content="Cancel"
                    CornerRadius="17"
                    FontSize="15"
                    Foreground="#FFFFFF"
                    Style="{StaticResource ButtonStyle}" />
            </StackPanel>
        </Grid>

    </Grid>
